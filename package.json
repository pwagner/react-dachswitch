{
  "name": "react-dachswitch",
  "version": "1.4.0",
  "description": "A country-code-based content filtering React component for the DACH region.",
  "main": "./dist/react-dachswitch.umd.cjs",
    "module": "./dist/react-dachswitch.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-dachswitch.js",
      "require": "./dist/react-dachswitch.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "homepage": "https://pwagner.github.io/react-dachswitch",
  "repository": {
    "url": "https://github.com/pwagner/react-dachswitch"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "vite build --config vite.config.lib.ts && tsc -p tsconfig.json --emitDeclarationOnly",
    "build:demo": "tsc && vite build",
    "deploy": "gh-pages -d build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "keywords": [
    "react",
    "dach",
    "switch",
    "filter",
    "content",
    "toggle",
    "germany",
    "austria",
    "switzerland",
    "ui",
    "component"
  ],
  "author": "Paul Wagner",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.19",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.0.0",
    "lucide-react": "^0.554.0",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  }
}
